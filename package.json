{
  "name": "wpthemestarter",
  "version": "1.0.0",
  "main": "webpack.config.js",
  "scripts": {
    "build": "rm -rf ./build && wp-scripts build --experimental-modules",
    "create": "cd src && npx @wordpress/create-block --no-plugin ",
    "create:interactive": "cd src && npx @wordpress/create-block   --template @wordpress/create-block-interactive-template  --no-plugin",
    "start": "wp-scripts start --experimental-modules",
    "format": "wp-scripts format",
    "lint:css": "wp-scripts lint-style",
    "lint:js": "wp-scripts lint-js",
    "packages-update": "wp-scripts packages-update",
    "plugin-zip": "wp-scripts plugin-zip",
    "dev": "webpack serve --config webpack.config.js --progress",
    "start:hot": "wp-scripts start --experimental-modules --hot",
    "start:block": "cd blocks/studiowac-block && wp-scripts start --experimental-modules --hot",
    "build:block": "cd blocks/studiowac-block && wp-scripts build --experimental-modules ",
    "start:navbar": "wp-scripts start --webpack-src-dir=blocks/studiowac-block --output-path=blocks/build/studiowac-block",
    "build:navbar": "wp-scripts build --webpack-src-dir=blocks/studiowac-block --output-path=blocks/build/studiowac-block",
    "watch": "wp-scripts start --experimental-modules --hot",
    "watch:scss": "sass --watch src/styles.scss dist/main.css",
    "build:scss": "sass src/styles.scss dist/main.css",
    "autoprefixer": "postcss dist/main.css --use autoprefixer -o dist/main.css",
    "build:css": "npm run build:scss && npm run autoprefixer",
    "build:lazy": "esbuild ./src/scripts/lazyload.ts --bundle --outfile=./dist/lazyload.js",
    "build:slider": "esbuild ./src/scripts/slider.ts --bundle --outfile=./dist/slider.js",
    "build:accordion": "esbuild ./src/scripts/accordion.ts --bundle --outfile=./dist/accordion.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/wordpress__block-editor": "^11.5.14",
    "@types/wordpress__blocks": "^12.5.14",
    "@types/wordpress__edit-post": "^7.5.7",
    "@types/wordpress__editor": "^13.6.8",
    "@wordpress/api-fetch": "^7.1.0",
    "@wordpress/block-editor": "^12.25.0",
    "@wordpress/blocks": "^12.34.0",
    "@wordpress/components": "^27.5.0",
    "@wordpress/edit-post": "^7.34.0",
    "@wordpress/editor": "^13.34.0",
    "@wordpress/env": "^9.9.0",
    "@wordpress/icons": "^9.48.0",
    "@wordpress/interactivity": "^5.6.0",
    "@wordpress/plugins": "^6.25.0",
    "@wordpress/scripts": "^27.8.0",
    "autoprefixer": "^10.4.19",
    "esbuild": "0.21.5",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "react": "^18.3.1",
    "sass": "^1.77.4",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0"
  }
}
